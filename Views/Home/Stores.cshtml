<script>




    (function($) {
        var ajaxRequestForSearch=null;
        $.fn.tipuedrop = function(options) {

            var set = $.extend( {

                'show'                   : 3,
                'speed'                  : 300,
                'newWindow'              : false,
                'mode'                   : 'json',
                'contentLocation'        : 'localhost:3000/api/products?q[name_cont]=nug'

            }, options);
            return this.each(function() {

                var tipuedrop_in = {
                    pages: []
                };
                $.ajaxSetup({
                    async: true
                });


                $(this).keyup(function(event) {
                    if (ajaxRequestForSearch != null)
                    {
                        ajaxRequestForSearch.abort();
                    }
                    getTipuedrop($(this));

                });

                function getTipuedrop($obj)
                {




                    if ($obj.val() && $obj.val().length>2)
                    {

                        var out = '<div class="tipue_drop_box"><div id="tipue_drop_wrapper">';


                        out += '<div class="tipue_drop_item"><div class="tipue_drop_left"></div><div class="tipue_drop_right">Loading</div></div>';



                        out += '</div></div>';

                        $('#tipue_drop_content').html(out);
                        $('#tipue_drop_content').fadeIn(set.speed);

                        ajaxRequestForSearch= $.ajax({
                            url: "/api/products?q[name_cont]="+$obj.val(),
                            type: 'GET',
                            success: function (data) {
                                var products=data["products"];
                                if(products.length>0){
                                    out = '<div class="tipue_drop_box" style="max-height: 300px ; overflow-y: scroll;"><div id="tipue_drop_wrapper">';
                                    for(var i=0;i<products.length;i++){
                                        master=products[i]["master"];
                                        productId=products[i]["id"]
                                        images=master["images"]
                                        productUrl="onclick=openProductsPopup('/products/"+productId+"')";
                                        if(images.length>0){
                                            out += '<a href="javascript:void(0);" '+productUrl+'  ><div class="tipue_drop_item"><div class="tipue_drop_left"><img src='+ images[0]["small_url"] +' class="tipue_drop_image"></div><div class="tipue_drop_right"> <h4>'+master["name"]+'</h4> </div><div class="tipue_drop_right"> <h4>'+master["display_price"]+'</h4> </div></div></a>';
                                        }else{
                                            out += '<a href="javascript:void(0);" '+productUrl+' ><div class="tipue_drop_item"><div class="tipue_drop_left"><img src="/assets/noimage/small-34061e2418f41e94f1a546c351a1a502cbe50d1de28a769c8054e797efdaa394.png" class="tipue_drop_image"></div><div class="tipue_drop_right"> <h4>'+master["name"]+'</h4> </div><div class="tipue_drop_right"> <h4>'+master["display_price"]+'</h4> </div></div></a>';
                                        }


                                    }
                                    submitForm='onclick=$("#searchFormNew").submit(); ';
                                    out += '<a href="javascript:void(0);" '+submitForm+' ><div class="tipue_drop_item"><div class="tipue_drop_left"></div><div class="tipue_drop_right"> <h4> View All Similar Results </h4> </div></div></a>';

                                    out += '</div></div>';
                                    $('#tipue_drop_content').html(out);
                                    $('#tipue_drop_content').fadeIn(set.speed);


                                }else{

                                    out = '<div class="tipue_drop_box" ><div id="tipue_drop_wrapper">';


                                    out += '<div class="tipue_drop_item"><div class="tipue_drop_left"></div><div class="tipue_drop_right">No Products Found !</div></div>';



                                    out += '</div></div>';

                                    $('#tipue_drop_content').html(out);
                                    $('#tipue_drop_content').fadeIn(set.speed);

                                }
                            }
                        });



                    }
                    else
                    {
                        $('#tipue_drop_content').fadeOut(set.speed);
                    }
                }

                $('html').click(function()
                {
                    $('#tipue_drop_content').fadeOut(set.speed);
                });

            });
        };

    })(jQuery);








</script>
